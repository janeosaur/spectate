<div class="add-team-player">
  <h1>Add Player to <%= params[:team_id] %></h1>
  <%= link_to "Back", :back %>
  <%= form_tag add_player_path, method: :post do %>
    <div class="">
      <%= collection_select :player, :id, @players, :id, :gamer_tag %>
    </div>
    <div class="">
      <%= submit_tag %>
    </div>
  <% end %>
</div>
